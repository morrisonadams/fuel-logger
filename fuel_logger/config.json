{
  "name": "Fuel Logger",
  "version": "1.1.0",
  "slug": "fuel_logger",
  "description": "Log fuel entries via Google Sheets and OpenAI",
  "arch": ["amd64", "armv7", "aarch64", "i386"],
  "startup": "services",
  "boot": "auto",
  "init": false,

  "build": {
    "context": ".",
    "dockerfile": "Dockerfile"
  },
  "ports": {
    "3001/tcp": 3001
  },
  "webui": "http://[HOST]:[PORT:3001]",
  "schema": {
    "OPENAI_API_KEY": "str",
    "GOOGLE_CLIENT_EMAIL": "str",
    "GOOGLE_PRIVATE_KEY": "str",
    "GOOGLE_SHEET_ID": "str"
  }
}
